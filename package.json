{
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "father-build && node ./scripts/clean-build.js",
    "bootstrap": "lerna bootstrap",
    "test": "npm run test:unit",
    "test:unit": "umi-test 'test.(j|t)s' --coverage",
    "test:watch": "umi-test 'test.(j|t)s' --coverage --watch",
    "predeploy": "npm run doc:build",
    "deploy": "now deploy ./dist -n sula --prod",
    "doc:dev": "cross-env DOC_ENV=dev dumi dev",
    "doc:build": "cross-env DOC_ENV=prod dumi build",
    "publish": "npm run build && lerna publish -m \"chore: publish\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umijs/sula.git"
  },
  "bugs": {
    "url": "https://github.com/umijs/sula/issues"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.1.0",
    "@umijs/fabric": "^2.0.8",
    "antd": "4.3.4",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "dumi": "^1.0.15",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "father-build": "^1.17.2",
    "lerna": "^3.20.2",
    "mockdate": "^2.0.5",
    "mockjs": "^1.1.0",
    "react-live": "^2.2.2",
    "sula": "^1.0.0-alpha.35",
    "@sula/charts": "^1.0.0-alpha.0",
    "umi-test": "^1.9.6"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "react-media": "^1.10.0",
    "tplstr": "^1.0.1"
  }
}
